{
    "name": "ticket-system",
    "version": "1.0.0",
    "description": "Discord ticket management system with web panel",
    "main": "server/index.js",
    "scripts": {
      "start": "node server/index.js",
      "server": "nodemon server/index.js",
      "client": "cd client && npm start",
      "dev": "concurrently \"npm run server\" \"npm run client\"",
      "install-client": "cd client && npm install",
      "install-server": "npm install",
      "install-all": "npm run install-server && npm run install-client",
      "build": "cd client && npm run build"
    },
    "keywords": [
      "discord",
      "tickets",
      "bot",
      "react"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
      "bcryptjs": "^2.4.3",
      "cors": "^2.8.5",
      "discord.js": "^14.14.1",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.2",
      "mongoose": "^8.0.0",
      "morgan": "^1.10.0",
      "passport": "^0.6.0",
      "passport-discord": "^0.1.4"
    },
    "devDependencies": {
      "concurrently": "^8.2.2",
      "nodemon": "^3.0.1"
    }
  }